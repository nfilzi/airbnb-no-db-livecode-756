<h1>Check out all our amazing flats!</h1>

<div class="cards">
  <% @flats.each do |flat| %>
    <div class="card-trip">
      <img src="<%= flat["imageUrl"] %>" />
      <div class="card-trip-infos">
        <div>
          <h2><%= flat["name"] %></h2>
        </div>
        <h2 class="card-trip-pricing"><%= flat["priceCurrency"] %><%= flat["price"] %> </h2>
      </div>
    </div>
  <% end %>
</div>
